{
    "name": "@lesnoypudge/utils",
    "version": "9.0.0",
    "description": "lesnoypudge's utils",
    "types": "./build/index.d.ts",
    "main": "./build/index.js",
    "exports": {
        ".": "./build/index.js",
        "./namespace": {
            "types": "./build/namespace.d.ts",
            "default": "./build/namespace.js"
        },
        "./package.json": "./package.json"
    },
    "type": "module",
    "files": [
        "build",
        "README.md"
    ],
    "scripts": {
        "nm:clear": "del-cli node_modules",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "publish": "npm publish --access public",
        "deploy": "git add . && git commit -m 'update' && git push && git status",
        "build": "del-cli build && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
        "formatter:config": "formatter-config",
        "lint:check": "eslint .",
        "lint:fix": "eslint . --fix",
        "typeCheck": "tsc -p tsconfig.json --noEmit"
    },
    "keywords": [
        "lesnoypudge",
        "utils"
    ],
    "author": "LesnoyPudge",
    "license": "ISC",
    "dependencies": {
        "@lesnoypudge/formatter-config": "^2.0.0",
        "@lesnoypudge/types-global-base": "^1.2.0",
        "@lesnoypudge/types-utils-base": "^1.1.0",
        "auto-bind": "^5.0.1",
        "defaults": "^3.0.0",
        "shallow-equal": "^3.1.0",
        "ts-functional-pipe": "^3.1.2",
        "unbuild": "^2.0.0"
    },
    "devDependencies": {
        "@lesnoypudge/eslint-base-config": "^1.9.0",
        "@types/jest": "^29.5.12",
        "@types/jsdom": "^21.1.7",
        "del-cli": "^5.1.0",
        "jest": "^29.7.0",
        "jsdom": "^24.1.0",
        "ts-jest": "^29.1.5",
        "tsc-alias": "^1.8.10",
        "tslib": "^2.6.3",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.6.0"
    }
}