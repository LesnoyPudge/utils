{
    "name": "@lesnoypudge/utils",
    "version": "7.7.0",
    "description": "lesnoypudge's utils",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "module",
    "files": [
        "build",
        "README.md"
    ],
    "scripts": {
        "nm:clear": "del-cli node_modules",
        "build": "del-cli build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json && node fix-imports.js && del-cli build/**/*.test.js && del-cli build/**/*.config.js",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "publish": "npm publish --access public",
        "deploy": "git add . && git commit -m 'update' && git push && git status",
        "build2": "del-cli build && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
        "build3": "unbuild",
        "build:clear": "del-cli build",
        "formatter:config": "formatter-config"
    },
    "keywords": [
        "lesnoypudge",
        "utils"
    ],
    "author": "LesnoyPudge",
    "license": "ISC",
    "dependencies": {
        "@lesnoypudge/formatter-config": "^1.0.5",
        "@lesnoypudge/types-global-base": "^1.2.0",
        "@lesnoypudge/types-utils-base": "^1.1.0",
        "auto-bind": "^5.0.1",
        "defaults": "^3.0.0",
        "shallow-equal": "^3.1.0",
        "unbuild": "^2.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/jsdom": "^21.1.6",
        "del-cli": "^5.1.0",
        "jest": "^29.7.0",
        "jsdom": "^24.1.0",
        "ts-jest": "^29.1.3",
        "tsc-alias": "^1.8.10",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.6.0"
    }
}