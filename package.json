{
    "name": "@lesnoypudge/utils",
    "version": "12.4.1",
    "description": "lesnoypudge's utils",
    "type": "module",
    "types": "./build/index.d.ts",
    "main": "./build/index.js",
    "exports": {
        ".": {
            "import": "./build/index.js",
            "require": "./build/index.js",
            "types": "./build/index.d.ts"
        },
        "./namespace": {
            "import": "./build/namespace.js",
            "require": "./build/namespace.js",
            "types": "./build/namespace.d.ts"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "nm:clear": "del-cli node_modules",
        "test:check": "vitest run",
        "test:watch": "vitest",
        "publish": "npm publish --access public",
        "deploy": "git add . && git commit -m 'update' && git push && git status",
        "build": "npm run test:check && del-cli build && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && node fix-imports.js",
        "build2": "node --experimental-strip-types build.ts",
        "build3": "tsc build.ts && node build.js && del-cli build.js",
        "formatter:config": "formatter-config",
        "lint:check": "eslint .",
        "lint:fix": "eslint . --fix",
        "type:check": "tsc --noEmit -p tsconfig.json",
        "type:watch": "tsc --noEmit -watch -p tsconfig.json"
    },
    "keywords": [
        "lesnoypudge",
        "utils"
    ],
    "author": "LesnoyPudge",
    "license": "ISC",
    "dependencies": {
        "@lesnoypudge/formatter-config": "^2.1.2",
        "@lesnoypudge/types-global-base": "^1.4.0",
        "@lesnoypudge/types-utils-base": "^3.3.0",
        "auto-bind": "^5.0.1",
        "defaults": "^3.0.0",
        "is-callable": "^1.2.7",
        "react-fast-compare": "^3.2.2",
        "shallow-equal": "^3.1.0",
        "ts-functional-pipe": "^3.1.2"
    },
    "devDependencies": {
        "@lesnoypudge/eslint-base-config": "^1.9.0",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.5.4",
        "del-cli": "^5.1.0",
        "jest": "^29.7.0",
        "jsdom": "^25.0.0",
        "ts-jest": "^29.2.5",
        "tsc-alias": "^1.8.10",
        "tslib": "^2.7.0",
        "typescript": "^5.6.2",
        "vite-tsconfig-paths": "^5.0.1",
        "vitest": "^2.0.5",
        "yup": "^1.4.0",
        "zod": "^3.23.8"
    }
}