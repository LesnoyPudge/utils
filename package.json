{
    "name": "@lesnoypudge/utils",
    "version": "6.0.1",
    "description": "lesnoypudge's utils",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "module",
    "files": [
        "build",
        "README.md"
    ],
    "scripts": {
        "nm:clear": "del-cli node_modules",
        "build:rollup": "rollup -c --bundleConfigAsCjs",
        "build": "del-cli build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json && node fix-imports.js && del-cli build/**/*.test.js && del-cli build/**/*.config.js",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "publish": "npm publish --access public",
        "deploy": "git add . && git commit -m 'update' && git push && git status",
        "lint": "npx @biomejs/biome lint ./src"
    },
    "keywords": [
        "lesnoypudge",
        "utils"
    ],
    "author": "LesnoyPudge",
    "license": "ISC",
    "dependencies": {
        "auto-bind": "^5.0.1",
        "defaults": "^3.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.7.3",
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/jest": "^29.5.12",
        "@types/jsdom": "^21.1.6",
        "del-cli": "^5.1.0",
        "esbuild": "^0.21.3",
        "jest": "^29.7.0",
        "jsdom": "^24.0.0",
        "rollup": "^4.17.2",
        "rollup-plugin-dts": "^6.1.0",
        "ts-essentials": "^10.0.0",
        "ts-jest": "^29.1.2",
        "tsc-alias": "^1.8.10",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.6.0"
    }
}