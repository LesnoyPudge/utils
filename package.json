{
    "name": "@lesnoypudge/utils",
    "version": "2.1.7",
    "description": "lesnoypudge's utils",
    "main": "build/src/index.js",
    "types": "build/src/index.d.ts",
    "type": "module",
    "files": [
        "build",
        "README.md"
    ],
    "scripts": {
        "dev": "",
        "build": "del-cli build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json && del-cli build/**/*.test.js && del-cli build/**/*.config.js",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "publish": "npm publish --access public",
        "deploy": "git add . && git commit -m 'update' && git push && git status",
        "lint": "npx @biomejs/biome lint ./src"
    },
    "keywords": [
        "lesnoypudge",
        "utils"
    ],
    "author": "LesnoyPudge",
    "license": "ISC",
    "dependencies": {
        "defaults": "^3.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.7.2",
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/jest": "^29.5.12",
        "@types/jsdom": "^21.1.6",
        "del-cli": "^5.1.0",
        "jest": "^29.7.0",
        "jsdom": "^24.0.0",
        "ts-essentials": "^10.0.0",
        "ts-jest": "^29.1.2",
        "tsc-alias": "^1.8.8",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.5.3"
    }
}