    "description": "lesnoypudge's utils",
    "type": "module",
    "types": "./build/index.d.ts",
    "main": "./build/index.js",
    "files": [
        "build"
    ],
    "scripts": {
        "test:check": "vitest --run",
        "test:watch": "vitest",
